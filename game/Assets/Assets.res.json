{
  "controls":
  {
    "res:type": "input",
    "events":
    {
      "up":
      [
        { "action": [ "up" ] },
        { "action": [ "w" ] },
        { "action": [ "joy-lup" ] },
        { "action": [ "joy-dup" ] }
      ],
      "down":
      [
        { "action": [ "down" ] },
        { "action": [ "s" ] },
        { "action": [ "joy-ldown" ] },
        { "action": [ "joy-ddown" ] }
      ],
      "left":
      [
        { "action": [ "left" ] },
        { "action": [ "a" ] },
        { "action": [ "joy-lleft" ] },
        { "action": [ "joy-dleft" ] }
      ],
      "right":
      [
        { "action": [ "right" ] },
        { "action": [ "d" ] },
        { "action": [ "joy-lright" ] },
        { "action": [ "joy-dright" ] }
      ],
      "action":
      [
        { "action": [ "return" ] },
        { "action": [ "space" ] },
        { "action": [ "joy-a" ] }
      ],
      "jump":
      [
        { "action": [ "x" ] },
        { "action": [ "space" ] },
        { "action": [ "joy-a" ] }
      ],
      "run":
      [
        { "action": [ "z" ] },
        { "action": [ "shift" ] },
        { "action": [ "joy-x" ] }
      ],
      "cancel":
      [
        { "action": [ "escape" ] }
      ],
      "pause":
      [
        { "action": [ "p" ] }
      ],
      "start":
      [
        { "action": [ "joy-start" ] }
      ]
    },
    "values":
    {
      "up":
      [
        { "action": "up" },
        { "action": "w" },
        { "action": "joy-lup" },
        { "action": "joy-dup" }
      ],
      "down":
      [
        { "action": "down" },
        { "action": "s" },
        { "action": "joy-ldown" },
        { "action": "joy-ddown" }
      ],
      "left":
      [
        { "action": "left" },
        { "action": "a" },
        { "action": "joy-lleft" },
        { "action": "joy-dleft" }
      ],
      "right":
      [
        { "action": "right" },
        { "action": "d" },
        { "action": "joy-lright" },
        { "action": "joy-dright" }
      ],
      "action":
      [
        { "action": "return" },
        { "action": "space" },
        { "action": "joy-a" }
      ],
      "jump":
      [
        { "action": "x" },
        { "action": "space" },
        { "action": "joy-a" }
      ],
      "run":
      [
        { "action": "z" },
        { "action": "shift" },
        { "action": "joy-x" }
      ]
    }
  },

  "gameDebugControls":
  {
    "res:type": "input",
    "events":
    {
      "cancelStepOneFrame":
      [
        { "action": [ "escape" ] }
      ],
      "stepOneFrame":
      [
        { "action": [ "1" ], "repeat": 0.25 }
      ],
      "showDebugUI":
      [
        { "action": [ "mouse-right" ] }
      ],
      "zoomPlayerCamera":
      [
        { "action": [ "4" ] }
      ]
    },
    "values":
    {
      "speedSlow":
      [
        { "action": "2" }
      ],
      "speedFast":
      [
        { "action": "3" }
      ]
    }
  }
}
